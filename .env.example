# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to git - it's in .gitignore

# Your Notion Integration Bearer Token
# Get this from: https://www.notion.so/my-integrations
NOTION_TOKEN=your_notion_bearer_token_here

# Your Notion Database ID
# Get this from your database URL: the 32-character string after the last slash
DATABASE_ID=your_database_id_here
